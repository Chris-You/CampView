<script type="text/javascript">

    var map;
    var markers=[];
    var infoWindows = [];
    var dataList = [];

    var center = {
        x: 0,
        y: 0
    };

    
    function resetMap() {
        
        markers=[];
        infoWindows = [];
        dataList = [];
        //console.log("mapInit");
	}


    function mapInit() {

        map = null;
        markers=[];
        infoWindows = [];
        dataList = [];
        //console.log("mapInit");

        navigator.geolocation.getCurrentPosition(function (position) {
            setMap(position.coords.latitude, position.coords.longitude);

            center.x = position.coords.longitude;
            center.y = position.coords.latitude;

            //console.log("position");
            //console.log(center);

            search(center);

            }
            , function (error) {
                console.log(error);
            });

        return center;
	}


    function mapSet() {

        if(!navigator.geolocation) {
            alert('Geolocation is not supported by your browser');
        }
        else
        {
            mapInit();
        }
    }

    function searchMap() {

        //console.log(map.getCenter());
        //search();
        center.x = map.getCenter().x;
        center.y = map.getCenter().y;

        search( center);
    }

    function hideMarker() {

        for (var i = 0; i < markers.length; i++) {
            markers[i].setMap(null);
            infoWindows[i].close();
        }

        markers = [];
        infoWindows = [];
	}

    function search(centerPos) {

        hideMarker();

        let facltDivNm = [];
        let induty = [];
        let sbrsCl = [];

        $("input:checkbox[name='facltDivNm']:checked").each(function () {
            facltDivNm.push($(this).val());
        });
        $("input:checkbox[name='induty']:checked").each(function () {
            induty.push($(this).val());
        });
        $("input:checkbox[name='sbrsCl']:checked").each(function () {
            sbrsCl.push($(this).val());
        });
        

        //data = "mapX=" + centerPos.x + "&mapY=" + centerPos.y + "&keyword=" + $("#keyword").val()
        var url = "/home/Search";
        /*
        data["mapX"] = centerPos.x;
        data["mapY"] = centerPos.y;
        data["keyword"] = $("#keyword").val();
        data["facltDivNm"] = facltDivNm;
        */
        var data = {
            "mapX": centerPos.x,
            "mapY": centerPos.y,
            "keyword": $("#keyword").val(),
            "facltDivNm": facltDivNm,
            "induty": induty,
            "sbrsCl": sbrsCl, 
        };

        //$("#divCampList").empty();
        var list = "";

        $.ajax({
            type: "post",
            //url: "/home/Search",
            url: url ,
            data: data,
            success: function (data) {
                //script = data.replace(/<script>(.*)<\/script>/, "$1"); // Remove tags
                //eval(script); // Execute javascript
                if (data.response.body.items.item.length > 0) {

                    dataList = data;
                    var cnt = (data.response.body.items.item.length < 20 ? data.response.body.items.item.length : 20);
                    if ($("#search").val() != "") {
                        cnt = data.response.body.items.item.length;
                    }

                    for (var i = 0; i < data.response.body.items.item.length; i++) {

                        var lng = data.response.body.items.item[i].mapX;
                        var lat = data.response.body.items.item[i].mapY;

                        setMarker(lat, lng, data.response.body.items.item[i].facltNm, i);

                        if (i < cnt) {

                            var visiable = ""
                            if (i >= 5) {
                                visiable = "non-visiable"
                            }

                            var imgUrl = data.response.body.items.item[i].firstImageUrl == "" ? "/img/no_image.png" : data.response.body.items.item[i].firstImageUrl;

                            list += '<div class="alert alert-info ' + visiable + '" >';
                            list += '    <div class="media">';
                            list += '        <a href="javascript:getClickListItem(' + i + ');">';
                            list += '        <div class="media-left">';

                            list += '            <img class="media-object" src="' + imgUrl + '" style="width: 80px; height: 60px;">';

                            list += '        </div>';
                            list += '        <div class="media-body">';
                            list += '        <h4 class="media-heading">' + data.response.body.items.item[i].facltNm + '</h4>';

                            var tel = "";
                            if (data.response.body.items.item[i].tel != "") {
                                //tel =    ' [Tel:' + data.response.body.items.item[i].tel + ']'
                            }

                            list += '        ' + data.response.body.items.item[i].addr1 + tel;

                            list += '        </div>';
                            list += '        </a>';
                            list += '        <div style="margin-left:100px;">';

                            if (data.response.body.items.item[i].facility != null && data.response.body.items.item[i].facility.length > 0) {

                                for (var j = 0; j < data.response.body.items.item[i].facility.length; j++) {
                                    list += '        <img src="/' + data.response.body.items.item[i].facility[j].url + '" alt=""  style="wisth:25px;height:25px;margin-right:5px;"/>'
								}

							}

                            list += '        </div>';
                            list += '    </div>';
                            list += '</div>';
                        }


                    }
                    /*
                    for (var i = 0; i < markers.length; i++)
                    {
                        markers[i].setMap(map);
                        naver.maps.Event.addListener(markers[i], 'click', getClickHandler(i));
                    }
                    */
                    // 검색은 첫번째 데이터로 이동
                    if ($("#search").val() != "") {
                        var center = new naver.maps.LatLng(data.response.body.items.item[0].mapY, data.response.body.items.item[0].mapX);

                        map.setCenter(center);
                    }

                    if (cnt > 5) {
                        // 더보기 버튼
                        list += '<div id="more">';
                        list += '  <button type="button" class="btn btn-default btn-lg btn-block" onclick="viewMore();">더보기(More)</button>';
                        list += '</div>';
                    }
                }
                else {
                    // 검색내역이 없습니다.
                    list += '<div style="text-align:center;">검색내역이 없습니다.</div>';

				}

                if (list != "") {
                    $("#divCampList").html(list);
                    $("#divCampList").show();
                    $("#divCampDtl").hide();
                }
            },
            error: function (data) {
                //alert("error: " + data.responseText);
            }
        });


        //console.log(markers.length);

        closeDiv("options");
        closeDiv('recentKeyword');
	}

    function setMap(lat, lng) {
        var mapOptions = {
            center: new naver.maps.LatLng(lat, lng),
            zoom: 10,
            //scaleControl: false,
            logoControl: false,
            //mapDataControl: false,
            //zoomControl: false,
        };

        //console.log(mapOptions);

        map = new naver.maps.Map('divMap', mapOptions);
	}

    function setMarker(lat, lng, facltNm, seq) {
        var marker = new naver.maps.Marker({
            position: new naver.maps.LatLng(lat, lng),
            map: map,
            /*
            icon: {
                url: '/img/camp_icon.png',
                size: new naver.maps.Size(22, 35)
                
            }
            */
        });

        markers.push(marker);

        var infoWindow = new naver.maps.InfoWindow({
            content: '<div style="width:150px;text-align:center;padding:10px;" onclick="closeInfo('+ seq +')"> <b>'+ facltNm +'</b></div>'
        });

        infoWindows.push(infoWindow);

        markers[seq].setMap(map);
        naver.maps.Event.addListener(markers[seq], 'click', getClickHandler(seq));

    }

    function closeInfo(seq) {
        infoWindows[seq].close();
	}

    // 해당 마커의 인덱스를 seq라는 클로저 변수로 저장하는 이벤트 핸들러를 반환합니다.
    function getClickHandler(seq) {

        return function (e) {

            getClickListItem(seq);
            
        }
    }

    function getClickListItem(seq) {

        var marker = markers[seq],
        infoWindow = infoWindows[seq];

        if (infoWindow.getMap()) {
            infoWindow.close();
        } else {
            infoWindow.open(map, marker);
        }

        setCampDetail(seq);
	}

