                                list += "    <div style='float:left;margin-left:0px;' >" + data[i].Value + "</div>"
                                list += "    <div style='margin-left:388px;' ><a href='javascript:deleteKeyword(\""+ data[i].Key +"\");'> X </a></div>"
                                list += "</li>"
                            }

